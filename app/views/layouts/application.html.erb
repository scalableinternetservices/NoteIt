</head>
<body tyle="padding-top: 50px; padding-bottom: 50px;
    text-align: center;
    background: url(<%= asset_path 'intro-bg.jpg' %>);

    background-size: 100%;
    color: #F1F3FA;
    ">

  <%= render 'partials/navbar' %>
    <br>
    <br>

  
  <% if user_signed_in? && !(request.env['PATH_INFO'] == edit_user_registration_path) && !(request.env['PATH_INFO'] == edit_user_password_path) %>
      
        <div style="margin: 0px 20px">
          <div class="row profile">
            <%= render 'partials/sidebar' %>
            <div class="col-md-9">
              <div class="profile-content">
                <%= yield %>
              </div>
            </div>
          </div>
        </div>

  <% else %>
    <%= yield %>
  <% end %>
  
  <br/>
  <!--<div >
     <%= debug(params) if Rails.env.development? %>
  </div>-->

</body>
<script>
     $(document).ready(function(){
        $('.dropdown-toggle').dropdown()
    });
</script>


</html>
